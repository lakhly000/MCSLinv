       RowBox[{"g", ",", "xL"}], "]"}]}], ",", 
     RowBox[{"{", 
      RowBox[{"xL", ",", 
       RowBox[{"-", "1"}], ",", "0"}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"z", ",", "0", ",", "Infinity"}], "}"}], ",", 
     RowBox[{"Assumptions", "\[Rule]", 
      RowBox[{"{", 
       RowBox[{"0", "<", "g", "<", "1"}], "}"}]}]}], "]"}]}]}]], "Input",
