Cell[TextData[StyleBox["Mathematica would not complete this integral with the \
chain rule, so it was added in by hand. (horizontally scaled by \
\[Mu]s+\[Mu]a).",
 FontColor->GrayLevel[0]]], "Text",
 CellChangeTimes->{{3.705930056848036*^9, 3.705930144781454*^9}, {
  3.7100853269856753`*^9, 3.7100853728327236`*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"probC", "[", 
   RowBox[{"t_", ",", "\[Mu]a_", ",", " ", "\[Mu]s_"}], "]"}], " ", ":=", " ", 
  RowBox[{
   FractionBox["\[Mu]s", 
    RowBox[{"\[Mu]s", "+", "\[Mu]a"}]], 
   RowBox[{"Integrate", "[", " ", 
    RowBox[{
     RowBox[{
      RowBox[{"Exp", "[", 
       RowBox[{"-", "z"}], "]"}], " ", 
      RowBox[{"Exp", "[", 
       RowBox[{
        RowBox[{"-", "z"}], "/", "xL"}], "]"}], 
      RowBox[{"(", 
       RowBox[{"1", "/", "2"}], ")"}]}], ",", 
     RowBox[{"{", 
      RowBox[{"xL", ",", "0", ",", "1"}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"z", ",", "0", ",", "t"}], "}"}], ",", 
     RowBox[{"Assumptions", "\[Rule]", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"t", ">", "0"}], ",", " ", 
        RowBox[{"\[Mu]a", " ", ">", " ", "0"}], ",", " ", 
        RowBox[{"\[Mu]s", " ", ">", " ", "0"}]}], "}"}]}]}], 
    "]"}]}]}]], "Input",
